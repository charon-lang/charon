extern fn printf(fmt: *char, ...): i32;

type Test struct {
    a: uint;
    b: uint;
}

let x: uint;
let test: Test;

fn main(): uint {

    x = 505;

    printf("%lu\n" as *char, x);

    x += 20;

    printf("%lu\n" as *char, x);

    test.a = 15;
    test.b = 20;

    printf("%lu\n" as *char, test.a + test.b);

    return 0;
}
