extern fn printf(fmt: *u8, ...): i32;
extern fn malloc(size: u64): *void;
extern fn free(ptr: *void);

fn main() {
    let brr = 0x80000 as *char;
    printf("%p\n" as *char, brr);

    let back = brr as uint;
    printf("%#lx\n" as *char, back);

    let data = malloc(16);

    *(data as *u8) = 100 as u8;

    free(data);
}