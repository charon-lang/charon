module A {
    module B {
        module C {
            extern fn printf(fmt: *char, ...): i32;

            type X struct {
                a: uint;
                b: uint;
            }
        }
    }
}

fn main(): u32 {

    let x: A::B::C::X;

    x.a = 5;
    x.b = 9;

    A::B::C::printf("%u\n", x.a + x.b);

    return 0 as u8;
}