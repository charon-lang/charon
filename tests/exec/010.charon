extern fn printf(fmt: *u8, ...): i32;

fn brr() {
    let x = 0;
    while(true) {
        x += 1;
        printf("<\n");
        if(x >= 5) return;
    }
}

fn main() {
    let x = 500 as *char;

    let i = 10;
    while(i > 0) {
        i -= 1;
        printf(">\n");
    }

    brr();
    printf("> the end\n");
}