extern fn printf(fmt: *u8, ...): i32;

fn test(x: u64) {
    printf("test > %i\n" as *char, x);
}

fn other_test(a: (u64, u64)) {
    printf("# %lu\n" as *char, a.1);

    printf("$ %lu\n" as *char, a.0);
    a.0 = 100;
    printf("%% %lu\n" as *char, a.0);
}

fn main(argc: i32, argv: *char) {
    test(500);
    other_test((25, 500));
}